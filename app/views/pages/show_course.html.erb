<div class="courses show">
	
	<table summary="Course detail view">
		<tr>
			<th>Name</th>
			<td><%= @course.name %></td>
		</tr>
		<tr>
			<th>Start Date</th>
			<td><%= @course.start_date %></td>
		</tr>
		<tr>
			<th>End Date</th>
			<td><%= @course.end_date %></td>
		</tr>
		<tr>
			<th>Class Times</th>
			<td><%= @course.course_times %></td>
		</tr>
		<tr>
			<th>Teacher ID</th>
			<td><%= @course.teacher_id %></td>
		</tr>
		<tr>
			<th>Price</th>
			<td><%= @course.price %></td>
		</tr>
		<tr>
			<th>Description</th>
			<td><%= @course.description %></td>
		</tr>
		<tr>
			<th>Address/th>
			<td><%= %></td>
		</tr>
	</table>
</div>