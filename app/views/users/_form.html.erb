<%= error_messages_for(@user) %>
 
<table summary = "User form fields">
	<tr>
		<th> <%= f.label(:name, "Name") %></th>
		<td><%= f.text_field(:name) %></td>
	</tr>

	<tr>
		<th><%= f.label(:created_at) %> </th>
		<td> <%= f.datetime_select(:created_at, :order => [:month, :day, :year]) %> </td>
	</tr>
	
</table>